<!DOCTYPE html><html><head>     {% block head %}    <meta charset="utf-8" />    <!--[if lt IE 9]><script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script><![endif]-->    <title>{% block title %}{{ title }}{% endblock %}</title>    <meta name="keywords" content="" />    <meta name="description" content="" />    <link rel="stylesheet" type="text/css" href="css/semantic/dist/semantic.min.css">    <link href="css/style.css" rel="stylesheet">    {% endblock %}</head><body>{% block search %}    <div class="style-header">        <div class="ui container">            <div class="ui grid">                <div class="thirteen wide column">                    <div class="ui right icon input">                        <input type="text" placeholder="Введите запрос...">                        <i class="search icon"></i>                    </div>                </div>                <div class="three wide column">                    <div class="ui buttons right floated">                        <a href="#" class="ui green button">Задать вопрос</a>{% if auth.isGuest %}<a href="/signup" class="ui blue button">Войти</a>{% else %}<a href="/logout" class="ui blue button">Выйти</a>{% endif %}                    </div>                </div>            </div>        </div>    </div>{% endblock %}<div class="ui container">    {% block menu %}        <div class="ui grid" id="container-menu">            <div class="three wide column">                <a href="/">                    <img src="images/logo.png" class="logo">                </a>            </div>            <div class="thirteen wide column">                <div class="ui buttons right floated">                    <a class="ui button">Вопросы</a>                    <a class="ui button">Тэги</a>                </div>            </div>        </div>    {% endblock %}    {% block content %}{% endblock %}</div><div class="ui inverted vertical footer segment">    <div class="ui center aligned container">        <div class="ui stackable inverted divided grid">            <div class="three wide column">                <h4 class="ui inverted header">Group 1</h4>                <div class="ui inverted link list">                    <a href="#" class="item">Link One</a>                    <a href="#" class="item">Link Two</a>                    <a href="#" class="item">Link Three</a>                    <a href="#" class="item">Link Four</a>                    <a href="#" class="item">Link One</a>                    <a href="#" class="item">Link Two</a>                    <a href="#" class="item">Link Three</a>                    <a href="#" class="item">Link Four</a>                </div>            </div>            <div class="three wide column">                <h4 class="ui inverted header">Group 2</h4>                <div class="ui inverted link list">                    <a href="#" class="item">Link One</a>                    <a href="#" class="item">Link Two</a>                    <a href="#" class="item">Link Three</a>                    <a href="#" class="item">Link Four</a>                    <a href="#" class="item">Link One</a>                    <a href="#" class="item">Link Two</a>                    <a href="#" class="item">Link Three</a>                    <a href="#" class="item">Link Four</a>                </div>            </div>            <div class="three wide column">                <h4 class="ui inverted header">Group 3</h4>                <div class="ui inverted link list">                    <a href="#" class="item">Link One</a>                    <a href="#" class="item">Link Two</a>                    <a href="#" class="item">Link Three</a>                    <a href="#" class="item">Link Four</a>                    <a href="#" class="item">Link One</a>                    <a href="#" class="item">Link Two</a>                    <a href="#" class="item">Link Three</a>                    <a href="#" class="item">Link Four</a>                </div>            </div>            <div class="seven wide column">                <h4 class="ui inverted header">Footer Header</h4>                <p>Extra space for a call to action inside the footer that could help re-engage users.</p>            </div>        </div>        <div class="ui inverted section divider"></div>        <img src="assets/images/logo.png" class="ui centered mini image">        <div class="ui horizontal inverted small divided link list">            <a class="item" href="#">Site Map</a>            <a class="item" href="#">Contact Us</a>            <a class="item" href="#">Terms and Conditions</a>            <a class="item" href="#">Privacy Policy</a>        </div>    </div></div><script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.2/jquery.min.js" type="text/javascript"></script><script src="css/semantic/dist/semantic.min.js"></script><script>    $('.ui.checkbox').checkbox();</script>{% for js in js_applets %}    {% if not js.header %}        <script type="text/javascript" src="{{ js.path }}"></script>    {% endif %}{% endfor %}{% if js_code %}    <script type="text/javascript">        $(document).ready(function() {            {% for code in js_code %}            {% if not code.header %}            {{ code.code | raw }}            {% endif %}            {% endfor %}        });    </script>{% endif %}</body></html>